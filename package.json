    "@emotion/styled": "^11.8.1",
    "@remix-run/react": "^1.3.3",
    "@remix-run/serve": "^1.3.3",
    "@remix-run/vercel": "^1.3.3",
    "framer-motion": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@remix-run/dev": "^1.3.3",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "commitlint": "^16.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^7.0.0",
